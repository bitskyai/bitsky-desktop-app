{
  "name": "dia-electron",
  "version": "0.0.1",
  "description": "Cross platform clients",
  "license": "Apache License 2.0",
  "readmeFilename": "README.md",
  "author": {
    "name": "munew.io",
    "email": "munewio@gmail.com"
  },
  "homepage": "https://munew.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/munew/dia-electron.git"
  },
  "bugs": {
    "url": "https://github.com/munew/dia-electron.git/issues"
  },
  "tags": [
    "Electron",
    "Munew",
    "DIA"
  ],
  "build": {
    "productName": "Munew",
    "appId": "org.munew",
    "files": [
      "node_modules/",
      "build/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "munew",
      "repo": "dia",
      "private": false
    }
  },
  "main": "./build/main.js",
  "dependencies": {
    "agendash": "^1.0.0",
    "ajv": "^6.10.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "circular-json": "^0.5.9",
    "compression": "^1.7.3",
    "csv-parse": "^4.6.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "fs-extra": "^7.0.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "semver": "^6.2.0",
    "server-destroy": "^1.0.1",
    "sqlite3": "^4.1.0",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "winston": "^3.2.1",
    "cookie-parser": "~1.4.4",
    "http-errors": "^1.6.3",
    "jade": "~1.11.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@hot-loader/react-dom": "^16.8.6",
    "connected-react-router": "^6.3.2",
    "core-js": "2",
    "devtron": "^1.4.0",
    "electron-log": "^3.0.8",
    "electron-debug": "^2.1.0",
    "electron-updater": "^4.0.6",
    "history": "^4.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.11"
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "electron": "^7.0.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "husky": "^3.0.9",
    "ndb": "^1.1.5",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  },
  "scripts": {
    "start": "npm run build && electron build/main.js",
    "build": "rimraf build && tsc && cp -rf app/engine-ui build/engine-ui && cp -rf app/soi build/soi && cp ./app/soi.html build/",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "check-dependencies": "ncu",
    "update-dependencies": "ncu -u",
    "postinstall": "install-app-deps"
  }
}
