  "scripts": {
    "less": "node ./tools/lessc.js",
    "lint:style": "stylelint ./app/less/*.less --fix",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -e \"node_modules/**/*.ts\" --fix",
    "lint:tests": "tslint ./tests/**/*.ts{,x} -c tslint.json --fix",
    "lint": "npm-run-all lint:*",
    "make": "electron-forge make",
    "package": "rimraf ./out && rimraf ./build && npm run copy-engine-soi && npm run tsc && electron-forge package",
    "parcel:build": "node ./tools/parcel.js",
    "parcel:watch": "cross-env WATCH=true node ./tools/parcel.js",
    "publish": "electron-forge publish",
    "start": "rimraf ./build && npm run copy-engine-soi && npm run tsc && electron-forge start",
    "watch": "cross-env WATCH=true && rimraf ./build && npm run copy-engine-soi && npm run tsc && electron-forge start",
    "copy-engine-soi": "mkdir -p build/app/ && cp -rf app/engine-ui build/app/engine-ui && cp -rf app/soi build/app/soi",
    "tsc": "node ./tools/tsc.js",
    "tsc-watch": "cross-env WATCH=true node ./tools/tsc.js",
    "check-dependencies": "ncu",
    "update-dependencies": "ncu -u"
  }
